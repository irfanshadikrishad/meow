    // brackets color
    "editorBracketHighlight.foreground1": "#84A59D",
    "editorBracketHighlight.foreground2": "#84A59D",
    "editorBracketHighlight.foreground3": "#84A59D",
    "editorBracketHighlight.foreground4": "#84A59D",
    "editorBracketHighlight.foreground5": "#84A59D",
    "editorBracketHighlight.foreground6": "#84A59D",

    // editor-group
    "editorGroupHeader.tabsBackground": "#131313",
