    "sideBarTitle.foreground": "#bbbbbb",
    "sideBar.background": "#131313",
    "sideBar.border": "#131313",
    "sideBarSectionHeader.foreground": "#f5f5f5",
    "sideBarSectionHeader.background": "#131313",
    "sideBarSectionHeader.border": "#131313",
    "focusBorder": "#131313",
    "list.activeSelectionBackground": "#131313",
    "list.activeSelectionForeground": "#f5f5f5",
    "list.inactiveSelectionBackground": "#f5f5f515",
    "list.inactiveSelectionForeground": "#f5f5f5",
    "list.errorForeground": "#E76161",
    // status-bar
    "statusBar.focusBorder": "#131313",
    "statusBar.foreground": "#bbbbbb",
    "statusBar.background": "#131313",
    "statusBar.border": "#131313",
    // tab
    "tab.inactiveBackground": "#131313",
    "tab.border": "#131313",
    // panel
    "panel.background": "#131313",
    "panel.border": "#131313"
